goto+if/while/do-while-for
goto可跳出多层嵌套

void main()
{
int i,sum = 0;
i = 1;
loop:if(i<=100)
{
sum = sum+i;
i++;
goto loop;
}
printf("%d\n",sum)
}

void main()
{
int i,sum=0;
i=1;
while(i<=100)
{
sum=sum+i;
i++;
}
printf("%d\n",sum)
}

void main()
{
int sum=0,i;
scanf("%d",&i);
do
{
sum=sum+i;
i++;
}while(i<=10);
printf("sum=%d",sum);
}
